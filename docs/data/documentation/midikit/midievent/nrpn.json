raits":[{"interfaceLanguage":"swift"}]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"NRPN"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"content","content":[{"level":2,"anchor":"overview","text":"Overview","type":"heading"},{"content":[{"inlineContent":[{"type":"text","text":""}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"In order to set or change the value of an Assignable Parameter (NRPN), the following"},{"type":"text","text":" "},{"type":"text","text":"occurs:"}],"type":"paragraph"},{"type":"orderedList","items":[{"content":[{"inlineContent":[{"text":"Two Control Change messages are sent using CC 99 (0x63) and 98 (0x62) to select the","type":"text"},{"type":"text","text":" "},{"text":"desired Assignable Parameter Number.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When setting the Assignable Parameter to a specific value, CC messages are sent to the"},{"type":"text","text":" "},{"type":"text","text":"Data Entry MSB controller (CC 6). If the selected Registered Parameter requires the LSB to"},{"text":" ","type":"text"},{"type":"text","text":"be set, another CC message is sent to the Data Entry LSB controller (CC 38)."}]}]},{"content":[{"inlineContent":[{"type":"text","text":"To make a relative adjustment to the selected Assignable Parameterâ€™s current value, use"},{"type":"text","text":" "},{"text":"the Data Increment or Data Decrement controllers (CCs 96 & 97).","type":"text"}],"type":"paragraph"}]}]},{"type":"paragraph","inlineContent":[{"text":"For registered Parameter Number use, see RPN (Registered Parameter Numbers).","type":"text"}]}],"type":"aside","name":"MIDI 1.0 Spec","style":"note"},{"content":[{"inlineContent":[{"text":"","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"In the MIDI 2.0 Protocol, Registered Controllers (RPN) and Assignable Controllers (NRPN)","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"use a single, unified message, making them much easier to use."}],"type":"paragraph"},{"inlineContent":[{"text":"As a result, CC 6, 38, 98, 99, 100, and 101 are not to be used in standalone CC messages,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"as the new MIDI 2.0 RPN\/NRPN UMP messages replace them."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Assignable Controllers (NRPNs) have no specific function and are available for any device"},{"text":" ","type":"text"},{"type":"text","text":"or application-specific function. Assignable Controllers map and translate directly to"},{"text":" ","type":"text"},{"text":"MIDI 1.0 Non-Registered Parameter Numbers (NRPN). Assignable Controllers are also","type":"text"},{"type":"text","text":" "},{"type":"text","text":"organized in 128 Banks (corresponds to NRPN MSB), with 128 controllers per Bank"},{"type":"text","text":" "},{"type":"text","text":"(corresponds to NRPN LSB)."}]}],"type":"aside","name":"MIDI 2.0 Spec","style":"note"},{"type":"paragraph","inlineContent":[{"text":"See Recommended Practise","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"https:\/\/www.midi.org\/specifications\/midi1-specifications\/midi-1-addenda\/response-to-data-increment-decrement-controllers"},{"type":"text","text":" "},{"text":"of the MIDI 1.0 Spec Addenda.","type":"text"}]}]}],"references":{"doc://MIDIKit/documentation/MIDIKit/MIDIEvent/NRPN/parameter":{"role":"symbol","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/NRPN\/parameter","type":"topic","abstract":[{"type":"text","text":"Non-Registered Parameter Number (Assignable Controller)."}],"title":"parameter","url":"\/documentation\/midikit\/midievent\/nrpn\/parameter","kind":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifi