ext":" ","kind":"text"},{"text":"activeSensing","kind":"identifier"},{"kind":"text","text":"("},{"text":"MIDIEvent","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent","preciseIdentifier":"s:11MIDIKitCore9MIDIEventO","kind":"typeIdentifier"},{"kind":"text","text":"."},{"identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/ActiveSensing","kind":"typeIdentifier","preciseIdentifier":"s:11MIDIKitCore9MIDIEventO13ActiveSensingV","text":"ActiveSensing"},{"text":")","kind":"text"}]}]},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Use of Active Sensing is optional for either receivers or transmitters. This byte (","type":"text"},{"type":"codeVoice","code":"0xFE"},{"type":"text","text":")"},{"text":" ","type":"text"},{"type":"text","text":"is sent every 300 ms (maximum) whenever there is no other MIDI data being transmitted. If"},{"text":" ","type":"text"},{"type":"text","text":"a device never receives Active Sensing it should operate normally. However, once the"},{"type":"text","text":" "},{"type":"text","text":"receiver recognizes Active Sensing ("},{"type":"codeVoice","code":"0xFE"},{"text":"), it then will expect to get a message of some","type":"text"},{"type":"text","text":" "},{"text":"kind every 300 milliseconds. If no messages are received within this time period the","type":"text"},{"text":" ","type":"text"},{"text":"receiver will assume the MIDI cable has been disconnected for some reason and should turn","type":"text"},{"type":"text","text":" "},{"text":"off all voices and return to normal operation. It is recommended that transmitters","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"transmit Active Sensing within 270ms and receivers judge at over 330ms leaving a margin of"},{"type":"text","text":" "},{"text":"roughly 10%.","type":"text"}]}],"style":"note","type":"aside","name":"MIDI 1.0 Spec"},{"type":"aside","style":"note","name":"Note","content":[{"inlineContent":[{"text":"Use of Active Sensing in modern MIDI devices is uncommon and the use of this","type":"text"},{"text":" ","type":"text"},{"text":"standard has been deprecated as of MIDI 2.0.","type":"text"}],"type":"paragraph"}]}]}],"references":{"doc://MIDIKit/documentation/MIDIKit":{"role":"collection","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit","kind":"symbol","abstract":[{"type":"text","text":"MIDIKit umbrella import that includes all MIDIKit modules."}],"type":"topic","title":"MIDIKit","url":"\/documentation\/midikit"},"doc://MIDIKit/documentation/MIDIKit/MIDIEvent/activeSensing(group:)":{"fragments":[{"kind":"keyword","text":"static"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"activeSensing"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"group"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt4","preciseIdentifier":"s:11MIDIKitCore5UInt4V"},{"text":") -> ","kind":"text"},{"text":"MIDIEvent","kind":"typeIdentifier","preciseIdentifier":"s:11MIDIKitCore9MIDIEventO"}],"title":"activeSensing(group:)","url":"\/documentation\/midikit\/midievent\/activesensing(group:)","abstract":[{"type":"text","text":"System Real-Time: Active Sensing"},{"type":"text","text":" "},{"type":"text","text":"(MIDI 1.0)"}],"identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/activeSensing(group:)","kind":"symbol","type":"topic","role":"symbol"},"doc://MIDIKit/documentation/MIDIKit/MIDIKitSMF":{"role":"collectionGroup","abstract":[{"type":"text","text":"Standard MIDI File extension for MIDIKit."}],"url":"\/documentation\/midikit\/midikitsmf","title":"MIDIKitSMF","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIKitSMF","kind":"article","type":"topic"},"doc://MIDIKit/documentation/MIDIKit/MIDIEvent/activeSensing(_:)":{"fragments":[{"kind":"keywor