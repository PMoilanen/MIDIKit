t\/MIDIEvent\/noteOn(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/NoteOn","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/noteOff(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/NoteOff","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/notePressure(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/NotePressure","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/cc(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/CC","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/pitchBend(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/PitchBend","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/pressure(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/Pressure","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/programChange(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/ProgramChange","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/rpn(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/RegisteredController","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/nrpn(_:)","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/NRPN","doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/AssignableController"],"generated":true}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/midikit\/midievent\/rpn"]}],"metadata":{"modules":[{"relatedModules":["MIDIKitCore"],"name":"MIDIKit"}],"extendedModule":"MIDIKitCore","roleHeading":"Structure","externalID":"s:11MIDIKitCore9MIDIEventO3RPNV","navigatorTitle":[{"kind":"identifier","text":"RPN"}],"role":"symbol","symbolKind":"struct","title":"MIDIEvent.RPN","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"kind":"identifier","text":"RPN"}]},"abstract":[{"text":"Channel Voice Message: RPN (Registered Parameter Number),","type":"text"},{"type":"text","text":" "},{"text":"also referred to as Registered Controller in MIDI 2.0.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"(MIDI 1.0 \/ MIDI 2.0)"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"platforms":["macOS"],"tokens":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"RPN","kind":"identifier"}]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Overview","anchor":"overview"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""}]},{"type":"paragraph","inlineContent":[{"text":"In order to set or change the value of a Registered Parameter (RPN), the following occurs:","type":"text"}]},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"Two Control Change messages are sent using CC 101 (0x65) and 100 (0x64) to select the"},{"type":"text","text":" "},{"type":"text","text":"desired Registered Parameter Number."}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"When setting the Registered Parameter to a specific value, CC messages are sent to the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Data Entry MSB controller (CC 6). If the selected Registered Parameter requires the LSB to"},{"type":"text","text":" "},{"type":"text","text":"be set, another CC message is sent to the Data Entry LSB controller (CC 38)."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"To make a relative adjustment to the selected Registered Parameterâ€™s current value, use","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the Data Increment or Data Decrement controllers (CCs 96 & 97)."}]}]}],"type":"orderedList"},{"type":"paragraph","inlineContent":[{"text":"Currently undefined RPN parameter numbers are all RESERVED for future MMA Definition.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"For custom Parameter Number use, see NRPN (Non-Registered Parameter Numbers).","type":"text"}]}],"style":"note","name":"MIDI 1.0 Spec","type":"aside"},{"style":"note","content":[{"inlineContent":[{"type":"text","text":""}],"type":"paragraph"},{"inlineContent":[{"text":"In the MIDI 2.0 Protocol, Registered Controllers (RPN) and Assignable Controllers (NRPN)","type":"text"},{"type":"text","text":" "},{"type":"text","text":"use a single, unified message, making them much easier to use."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"As a result, CC 6, 38, 98, 99, 100, and 101 are not to be used in standalone CC messages,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"as the new MIDI 2.0 RPN\/NRPN UMP messages replace them."}]},{"inlineContent":[{"text":"Registered Controllers (RPNs) have specific functions defined by MMA\/AMEI specifications.","type":"text"},{"text":" ","type":"text"},{"text":"Registered Controllers map and translate directly to MIDI 1.0 Registered Parameter Numbers","type":"text"},{"type":"text","text":" "},{"type":"text","text":"(RPN, see Appendix D.2.3) and use the same definitions as MMA\/AMEI approved RPN messages."},{"type":"text","text":" "},{"type":"text","text":"Registered Controllers are organized in 128 Banks (corresponds to RPN MSB), with 128"},{"text":" ","type":"text"},{"text":"controllers per Bank (corresponds to RPN LSB).","type":"text"}],"type":"paragraph"}],"name":"MIDI 2.0 Spec","type":"aside"},{"content":[{"inlineContent":[{"text":"See Recommended Practise","type":"text"},{"text":" ","type":"text"},{"identifier":"https:\/\/www.midi.org\/specifications\/midi1-specifications\/midi-1-addenda\/response-to-data-increment-decrement-controllers","type":"reference","isActive":true},{"text":" ","type":"text"},{"type":"text","text":"of the MIDI 1.0 Spec Addenda."}],"type":"paragraph"}],"style":"note","type":"aside","name":"Note"}]}],"references":{"https://www.midi.org/specifications/midi1-specifications/midi-1-addenda/response-to-data-increment-decrement-controllers":{"titleInlineContent":[{"text":"RP-018","type":"text"}],"title":"RP-018","type":"link","url":"https:\/\/www.midi.org\/specifications\/midi1-specifications\/midi