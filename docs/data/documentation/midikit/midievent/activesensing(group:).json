nd":"externalParam","text":"group"},{"kind":"text","text":": "},{"identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/UInt4","text":"UInt4","kind":"typeIdentifier","preciseIdentifier":"s:11MIDIKitCore5UInt4V"},{"text":" = 0x0) -> ","kind":"text"},{"preciseIdentifier":"s:11MIDIKitCore9MIDIEventO","text":"MIDIEvent","kind":"typeIdentifier","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"UMP Group ("},{"type":"codeVoice","code":"0x0 ... 0xF"},{"text":")","type":"text"}]}],"name":"group"}]},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"content":[{"inlineContent":[{"type":"text","text":""}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Use of Active Sensing is optional for either receivers or transmitters. This byte ("},{"type":"codeVoice","code":"0xFE"},{"type":"text","text":")"},{"type":"text","text":" "},{"type":"text","text":"is sent every 300 ms (maximum) whenever there is no other MIDI data being transmitted. If"},{"type":"text","text":" "},{"type":"text","text":"a device never receives Active Sensing it should operate normally. However, once the"},{"type":"text","text":" "},{"text":"receiver recognizes Active Sensing (","type":"text"},{"code":"0xFE","type":"codeVoice"},{"text":"), it then will expect to get a message of some","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"kind every 300 milliseconds. If no messages are received within this time period the"},{"text":" ","type":"text"},{"text":"receiver will assume the MIDI cable has been disconnected for some reason and should turn","type":"text"},{"type":"text","text":" "},{"type":"text","text":"off all voices and return to normal operation. It is recommended that transmitters"},{"type":"text","text":" "},{"text":"transmit Active Sensing within 270ms and receivers judge at over 330ms leaving a margin of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"roughly 10%."}]}],"name":"MIDI 1.0 Spec","type":"aside","style":"note"},{"style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Use of Active Sensing in modern MIDI devices is uncommon and the use of this"},{"type":"text","text":" "},{"type":"text","text":"standard has been deprecated as of MIDI 2.0."}]}],"type":"aside","name":"Note"}]}],"variants":[{"paths":["\/documentation\/midikit\/midievent\/activesensing(group:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent\/activeSensing(group:)","interfaceLanguage":"swift"},"kind":"symbol","references":{"doc://MIDIKit/documentation/MIDIKit":{"kind":"symbol","type":"topic","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit","url":"\/documentation\/midikit","abstract":[{"type":"text","text":"MIDIKit umbrella import that includes all MIDIKit modules."}],"title":"MIDIKit","role":"collection"},"doc://MIDIKit/documentation/MIDIKit/UInt4":{"navigatorTitle":[{"kind":"identifier","text":"UInt4"}],"type":"topic","url":"\/documentation\/midikit\/uint4","kind":"symbol","role":"symbol","abstract":[{"text":"A 4-bit unsigned integer value type used in ","type":"text"},{"code":"MIDIKit","type":"codeVoice"},{"type":"text","text":"."}],"identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/UInt4","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"text":"UInt4","kind":"identifier"}],"title":"UInt4"},"doc://MIDIKit/documentation/MIDIKit/MIDIEvent":{"url":"\/documentation\/midikit\/midievent","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIEvent","type":"topic","abstract":[{"type":"text","text":"An individual MIDI Event."}],"role":"symbol","kind":"symbol","fragments":[{"kind":"keyword","text":"enum"},{"text":" ","kind":"text"},{"kind":"identifier","text":"MIDIEvent"}],"tit