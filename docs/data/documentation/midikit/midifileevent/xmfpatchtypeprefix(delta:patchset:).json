latforms":["macOS"],"tokens":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"xmfPatchTypePrefix"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"delta"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent","text":"MIDIFileEvent","kind":"typeIdentifier"},{"text":".","kind":"text"},{"preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO9DeltaTimeO","kind":"typeIdentifier","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent\/DeltaTime","text":"DeltaTime"},{"text":" = .none, ","kind":"text"},{"kind":"externalParam","text":"patchSet"},{"text":": ","kind":"text"},{"text":"MIDIFileEvent","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent","preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO","kind":"typeIdentifier"},{"text":".","kind":"text"},{"text":"XMFPatchTypePrefix","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent\/XMFPatchTypePrefix","kind":"typeIdentifier","preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO18XMFPatchTypePrefixV"},{"text":".","kind":"text"},{"text":"PatchSet","kind":"typeIdentifier","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent\/XMFPatchTypePrefix\/PatchSet-swift.enum","preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO18XMFPatchTypePrefixV8PatchSetO"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","preciseIdentifier":"s:10MIDIKitSMF13MIDIFileEventO","identifier":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent","text":"MIDIFileEvent"}]}],"kind":"declarations"},{"kind":"content","content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"style":"note","content":[{"inlineContent":[{"type":"text","text":""}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"XMF Type 0 and Type 1 files contain Standard MIDI Files (SMF). Each SMF Track in such XMF"},{"type":"text","text":" "},{"type":"text","text":"files may be designated to use either standard General MIDI 1 or General MIDI 2"},{"type":"text","text":" "},{"text":"instruments supplied by the player, or custom DLS instruments supplied via the XMF","type":"text"},{"text":" ","type":"text"},{"text":"file. This document defines a new SMF Meta-Event to be used for this purpose.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"In a Type 0 or Type 1 XMF File, this meta-event specifies how to interpret subsequent","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Program Change and Bank Select messages appearing in the same SMF Track: as General"},{"text":" ","type":"text"},{"type":"text","text":"MIDI 1, General MIDI 2, or DLS. In the absence of an initial XMF Patch Type Prefix"},{"type":"text","text":" "},{"text":"Meta-Event, General MIDI 1 (instrument set and system behavior) is chosen by default.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"In a Type 0 or Type 1 XMF File, no SMF Track may be reassigned to a different instrument","type":"text"},{"type":"text","text":" "},{"type":"text","text":"set (GM1, GM2, or DLS) at any time. Therefore, this meta-event should only be processed if"},{"type":"text","text":" "},{"type":"text","text":"it appears as the first message in an SMF Track; if it appears anywhere else in an SMF"},{"text":" ","type":"text"},{"text":"Track, it must be ignored.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"See ","type":"text"},{"type":"reference","identifier":"https:\/\/www.midi.org\/specifications\/file-format-specifications\/standard-midi-files\/xmf-patch-type-prefix-meta-event","isActive":true},{"type":"text","text":"."}]}],"name":"Standard MIDI File 1.0 Spec","type":"aside"}]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MIDIKit\/documentation\/MIDIKit\/MIDIFileEvent\